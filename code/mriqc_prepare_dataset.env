# mriqc_prepare_dataset.env - Configuration for MRIQC preprocessing

# Raw data source (SAMPLE is appended to this URL)
RAW_STORE_BASE=https://github.com/OpenNeuroDatasets

# MRIQC settings
MRIQC_VERSION=24.0.2
NTHREADS=1
MEM_MB=3000
MODALITIES=

# Container source
CONTAINER_STORE=https://github.com/ReproNim/containers.git

# Scratch directory for MRIQC working files
SCRATCH_DIR=/tmp
